# PostgreSQL connection settings for social_media_database
# The orchestrator will map these for each environment.
POSTGRES_HOST=localhost
POSTGRES_PORT=5000
POSTGRES_USER=appuser
POSTGRES_PASSWORD=dbuser123
POSTGRES_DB=myapp
# Optional unified URL. If provided, backend may prefer this over individual vars.
POSTGRES_URL=postgresql://appuser:dbuser123@localhost:5000/myapp

# Frontend/Backend variables (not used by DB container directly but documented for clarity)
REACT_APP_API_BASE=http://localhost:3010
REACT_APP_BACKEND_URL=http://localhost:3010
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:3010
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=true
REACT_APP_LOG_LEVEL=debug
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_FEATURE_FLAGS=enable_new_analytics
REACT_APP_EXPERIMENTS_ENABLED=false
